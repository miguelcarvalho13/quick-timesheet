{
  "name": "quick-timesheet",
  "private": true,
  "version": "0.2.0",
  "homepage": "https://miguelcarvalho13.github.io/quick-timesheet",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "tsc && vite build",
    "deploy": "gh-pages -d dist",
    "lint": "npm-run-all --aggregate-output --parallel lint:*",
    "lint:js": "eslint . --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:prettier": "prettier -c .",
    "lint:spell": "cspell lint '**/*.{css,md,js,json,jsx,ts,tsx}' --no-progress",
    "lint:ts": "tsc --noEmit",
    "predeploy": "yarn build",
    "preview": "vite preview --host 0.0.0.0",
    "test": "vitest"
  },
  "dependencies": {
    "dayjs": "^1.11.13",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.13",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "cspell": "^8.17.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "gh-pages": "^6.2.0",
    "jsdom": "^25.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "vite": "^6.0.6",
    "vitest": "^2.1.8"
  },
  "resolutions": {
    "strip-ansi": "^6.0.1"
  }
}
