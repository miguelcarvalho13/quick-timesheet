{
  "name": "quick-timesheet",
  "private": true,
  "version": "0.2.0",
  "homepage": "https://miguelcarvalho13.github.io/quick-timesheet",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "tsc && vite build",
    "deploy": "gh-pages -d dist",
    "lint": "npm-run-all --aggregate-output --parallel lint:*",
    "lint:js": "eslint . --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:prettier": "prettier -c .",
    "lint:spell": "cspell lint '**/*.{css,md,js,json,jsx,ts,tsx}' --no-progress",
    "lint:ts": "tsc --noEmit",
    "predeploy": "yarn build",
    "preview": "vite preview --host 0.0.0.0",
    "test": "vitest"
  },
  "dependencies": {
    "dayjs": "^1.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "cspell": "^8.0.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "gh-pages": "^6.1.0",
    "jsdom": "^23.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.3.2",
    "vite": "^5.0.3",
    "vitest": "^0.34.6"
  }
}
