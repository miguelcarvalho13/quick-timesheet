{
  "name": "quick-timesheet",
  "private": true,
  "version": "0.2.0",
  "homepage": "https://miguelcarvalho13.github.io/quick-timesheet",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "tsc && vite build",
    "deploy": "gh-pages -d dist",
    "lint": "npm-run-all --aggregate-output --parallel lint:*",
    "lint:js": "eslint . --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:prettier": "prettier -c .",
    "lint:spell": "cspell lint '**/*.{css,md,js,json,jsx,ts,tsx}' --no-progress",
    "lint:ts": "tsc --noEmit",
    "predeploy": "yarn build",
    "preview": "vite preview --host 0.0.0.0",
    "test": "vitest"
  },
  "dependencies": {
    "dayjs": "^1.11.18",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.16",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.13",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^5.1.0",
    "cspell": "^9.2.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^6.1.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "gh-pages": "^6.3.0",
    "jsdom": "^27.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.15",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vitest": "^4.0.2"
  },
  "resolutions": {
    "strip-ansi": "^6.0.1"
  }
}
